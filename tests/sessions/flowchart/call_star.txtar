version: v1

imports:
  - name: foo
    path: "foo.star"

nodes:
    - name: main
      action:
        call:
          target: imports.foo.say
          args:
            sound: "'meow'"
      goto: nodes.end

    - name: end
      action:
        print:
          value: "states.results.main"

-- foo.star --
def say(sound):
  return "{}, world!".format(sound)
