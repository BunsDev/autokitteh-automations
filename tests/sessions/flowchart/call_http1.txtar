-- main.flowchart.yaml main --
version: v1

options:
  interpolate_all_strings: true
  default_interpolation: p

nodes:
  - name: main
    action:
      call:
        target: globals.http.get
        args:
          # getting these from the session inputs
          url: "http://{{ args.ak_addr }}/uptime"
    result: ^cel^ states.nodes.main.body.json
    goto: nodes.end

  - name: end
    action:
      print:
        value: ^cel^ states.nodes.main.seconds >= 0
