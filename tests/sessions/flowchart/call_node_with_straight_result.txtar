meow, world!

-- main.flowchart.yaml main --
version: v1

nodes:
    - name: main
      action:
        call:
          target: nodes.phrase
          args:
            sound: "'meow'"
      goto: nodes.end

    - name: phrase
      result: "args.sound + ', world!'"

    - name: end
      action:
        print:
          value: "states.results.main"
