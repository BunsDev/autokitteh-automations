-- main.flowchart.yaml main --
version: v1

nodes:
  - name: main
    action:
      foreach:
        items: ["1st", "2nd", "3rd"]
        call:
          target: nodes.worker
    goto: nodes.end

  - name: worker
    result: "^^ 'meow ' + states.nodes.main.item"

  - name: end
    action:
      print:
        value: "^^ states.nodes.main"
  