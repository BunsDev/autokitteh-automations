["meow 1st","meow 2nd","meow 3rd"]

-- main.flowchart.yaml main --
version: v1

imports:
  - name: foo
    path: "foo.star"

nodes:
  - name: main
    action:
      call:
        target: "imports.foo.bar"
    goto: nodes.loop

  - name: loop
    action:
      foreach:
        items: "^^ states.nodes.main"
        call:
          target: nodes.worker
    goto: nodes.end

  - name: worker
    result: "^^ 'meow ' + states.nodes.loop.item"

  - name: end
    action:
      print:
        value: "^^ states.nodes.loop"

-- foo.star --
def bar():
  return ["1st", "2nd", "3rd"]
