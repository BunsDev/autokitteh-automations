-- main.flowchart.yaml main --
version: v1

imports:
    - name: sub
      path: sub.flowchart.yaml

nodes:
    - name: main
      action:
        call:
          target: imports.sub.foo
      goto: nodes.end

    - name: end
      action:
        print:
          value: "'done!'"

-- sub.flowchart.yaml --
version: v1

nodes:
    - name: foo
      action:
        print:
          value: "'meow'"
      goto: nodes.bar

    - name: bar
      action:
        print:
          value: "'bub and bob say buh bye!'"
