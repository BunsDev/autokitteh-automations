-- main.flowchart.yaml main --
version: v1

imports:
  - name: foo
    path: "foo.star"

nodes:
    - name: main
      action:
        call:
          target: imports.foo.uptime
          args:
            # getting these from the session inputs
            host: ^^ args.ak_addr
      result: ^^ states.nodes.main.body.json
      goto: nodes.end

    - name: end
      action:
        print:
          value: "^^ states.nodes.main.seconds >= 0"

-- foo.star --
def uptime(host):
  return http.get("http://{}/uptime".format(host))
