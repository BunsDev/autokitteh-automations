-- main.flowchart.yaml main --
version: v1

nodes:
    - name: main
      action:
        call:
          target: nodes.say
          args:
            sound: "meow"
          async: true
      goto: nodes.end

    - name: say
      action:
        print:
          value: ^^ "args.sound + ', world!'"

    - name: end
      action:
        print:
          value: ^^ states.nodes.main.startsWith('ses_')

